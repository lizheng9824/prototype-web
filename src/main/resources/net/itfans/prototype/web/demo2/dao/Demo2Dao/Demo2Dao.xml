<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--(1)-->
<mapper namespace="net.itfans.prototype.web.demo2.dao.Demo2Dao">
    <insert id="insertUser" parameterType="net.itfans.prototype.web.demo2.entity.UserEntity">
        INSERT INTO users
          (user_id, password, insert_date, update_date)
        VALUES
          (#{userId}, #{password}, #{insertDate}, #{updateDate});
    </insert>
</mapper>